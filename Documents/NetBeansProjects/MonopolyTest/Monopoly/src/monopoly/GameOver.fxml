<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane id="AnchorPane" prefHeight="447.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/9" fx:controller="monopoly.GameOverController">
   <children>
      <ImageView fitHeight="461.0" fitWidth="600.0" pickOnBounds="true">
         <image>
            <Image url="@../starTrekBackground.jpg" />
         </image>
      </ImageView>
      <Label alignment="CENTER" layoutX="117.0" layoutY="45.0" prefHeight="117.0" prefWidth="366.0" text="GAME OVER" textFill="WHITE">
         <font>
            <Font name="Century Gothic" size="58.0" />
         </font>
      </Label>
      <Label fx:id="lblWinner" alignment="CENTER" layoutX="149.0" layoutY="135.0" prefHeight="96.0" prefWidth="301.0" textFill="WHITE">
         <font>
            <Font name="Century Gothic" size="32.0" />
         </font>
      </Label>
      <Button fx:id="bntRestart" layoutX="123.0" layoutY="287.0" mnemonicParsing="false" onAction="#restart" prefHeight="82.0" prefWidth="103.0" text="RESTART">
         <font>
            <Font name="Century Gothic" size="17.0" />
         </font>
      </Button>
      <Button fx:id="btnExit" layoutX="379.0" layoutY="287.0" mnemonicParsing="false" onAction="#exit" prefHeight="82.0" prefWidth="103.0" text="EXIT">
         <font>
            <Font name="Century Gothic" size="17.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
