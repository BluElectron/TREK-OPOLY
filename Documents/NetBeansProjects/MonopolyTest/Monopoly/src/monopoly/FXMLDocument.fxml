            <Image url="@../animated-dice-image-0015.gif" />
         </image></ImageView>
      <ImageView fx:id="imgPlayer1" fitHeight="41.0" fitWidth="37.0" layoutX="520.0" layoutY="535.0" pickOnBounds="true" preserveRatio="true" />
      <ImageView fx:id="imgPlayer2" fitHeight="41.0" fitWidth="37.0" layoutX="514.0" layoutY="532.0" pickOnBounds="true" preserveRatio="true" />
      <ImageView fx:id="dice1" fitHeight="52.0" fitWidth="58.0" layoutX="614.0" layoutY="377.0" pickOnBounds="true" preserveRatio="true" />
      <ImageView fx:id="dice2" fitHeight="52.0" fitWidth="58.0" layoutX="614.0" layoutY="436.0" pickOnBounds="true" preserveRatio="true" />
      <Label fx:id="lblTurn" layoutX="623.0" layoutY="229.0" prefHeight="70.0" prefWidth="322.0" textFill="WHITE">
         <font>
            <Font name="Century Gothic" size="12.0" />
         </font>
      </Label>
      <Label fx:id="lblPlayer1" layoutX="851.0" layoutY="60.0" text="PLAYER 1:" textFill="WHITE">
         <font>
            <Font name="Century Gothic" size="12.0" />
         </font>
      </Label>
      <Label fx:id="lblMoney1" layoutX="825.0" layoutY="90.0" prefHeight="15.0" prefWidth="111.0" textFill="WHITE">
         <font>
            <Font name="Century Gothic" size="12.0" />
         </font>
      </Label>
      <Label fx:id="lblPlayer2" layoutX="851.0" layoutY="127.0" text="PLAYER 2: " textFill="WHITE">
         <font>
            <Font name="Century Gothic" size="12.0" />
         </font>
      </Label>
      <Label fx:id="lblMoney2" layoutX="825.0" layoutY="154.0" prefHeight="15.0" prefWidth="111.0" textFill="WHITE">
         <font>
            <Font name="Century Gothic" size="12.0" />
         </font>
      </Label>
