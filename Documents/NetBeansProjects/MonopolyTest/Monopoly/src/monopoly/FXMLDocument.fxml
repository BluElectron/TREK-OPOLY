<AnchorPane id="AnchorPane" fx:id="gamePane" prefHeight="585.0" prefWidth="973.0" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="monopoly.FXMLDocumentController">
   <children>
      <ImageView fx:id="imgTrekPaper" fitHeight="601.0" fitWidth="376.0" layoutX="596.0" pickOnBounds="true">
