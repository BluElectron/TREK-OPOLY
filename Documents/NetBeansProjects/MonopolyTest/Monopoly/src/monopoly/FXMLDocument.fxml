            <Image url="@../trekPaper.png" />
         </image>
      </ImageView>
      <TextArea fx:id="txtRoll" layoutX="613.0" layoutY="69.0" prefHeight="57.0" prefWidth="101.0" />
      <Button fx:id="btnRoll" layoutX="642.0" layoutY="29.0" mnemonicParsing="false" onAction="#newRoll" text="ROLL">
         <font>
            <Font name="Century Gothic" size="12.0" />
         </font></Button>
      <ImageView fx:id="imgBoard" fitHeight="596.0" fitWidth="598.0" layoutY="2.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../MonopolyBoard.jpg" />
         </image>
      </ImageView>
      <ImageView fx:id="diceRoll" fitHeight="78.0" fitWidth="111.0" layoutX="122.0" layoutY="412.0" pickOnBounds="true" preserveRatio="true">
